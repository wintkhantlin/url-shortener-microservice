[
  {
    "id": "management-api",
    "upstream": {
      "url": "http://management:8001",
      "strip_path": "/api/management"
    },
    "match": {
      "url": "http://localhost:4455/api/management/<**>",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "DELETE",
        "PATCH",
        "OPTIONS"
      ]
    },
    "authenticators": [
      {
        "handler": "cookie_session"
      }
    ],
    "authorizer": {
      "handler": "allow"
    },
    "mutators": [
      {
        "handler": "header"
      }
    ]
  },
  {
